    function TatThongBao(id){ // tắt thông báo
      if($('#Tat'+id).is(':checked') == true){
        $.cookie("An"+id, 1, { expires : 1 });
      }
    };
    function BatThongBao(id){ //bật thông báo
      if (typeof $.cookie('An'+id) === 'undefined'){ 
        $('#'+id).show();
      }
    }
    const Toast = Swal.mixin({
        toast: true,
        position: "top-end",
        showConfirmButton: false,
        timer: 5000,
        timerProgressBar: true,
        didOpen: (toast) => {
            toast.onmouseenter = Swal.stopTimer;
            toast.onmouseleave = Swal.resumeTimer;
        }
    });
    function ThongBaoNhanh(type, tieude, noidung, thoigian){
        Toast.fire({
            icon: type,
            title: noidung
        });
    }
    function ThongBao(type, tieude, noidung){
      Swal.fire({
        title: tieude,
        text: noidung,
        icon: type
      });
    }
    function ReloadTable(id){ //Làm mới DataTable
      $("#Loading").show();
      $("#Table"+id).load("Data_"+id, function( response, status, xhr ) {
        if ( status == "error" ) {
          $("#Loading").hide();
          ThongBao("error", "Thất bại!", "Đã có lỗi xảy ra, thử lại sau!");
          return;
        }
        window.loaded = true;
        ThongBao("success", "Thành công!", "Đã làm mới bảng dữ liệu");
        $("#Loading").hide();
      });
    }
    function copy(text) { // Copy
      ThongBao("success", "Thành công!", "Đã sao chép: "+text, "");
      if (window.clipboardData && window.clipboardData.setData) {
          // Internet Explorer-specific code path to prevent textarea being shown while dialog is visible.
          return window.clipboardData.setData("Text", text);
      }else if (document.queryCommandSupported && document.queryCommandSupported("copy")) {
        var textarea = document.createElement("textarea");
        textarea.textContent = text;
        textarea.style.position = "fixed";  // Prevent scrolling to bottom of page in Microsoft Edge.
        document.body.appendChild(textarea);
        textarea.select();
        try {
            return document.execCommand("copy");  // Security exception may be thrown by some browsers.
        }catch (ex) {
            console.warn("Copy to clipboard failed.", ex);
            return prompt("Copy to clipboard: Ctrl+C, Enter", text);
        }finally {
            document.body.removeChild(textarea);
        }
      }
    }
