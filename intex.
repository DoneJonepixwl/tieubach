<script>
    function ShowPassword(id) {
      var x = document.getElementById(id);
      if (x.type === "password") {
        x.type = "text";
      } else {
        x.type = "password";
      }
    }
    function OpenReg(){
        $("#Landing").hide();
        $("#DangNhap").hide();
        $("#DangKy").show();
        $("#LayMatKhau").hide();
    }
    function OpenLanding(){
        $("#Landing").show();
        $("#DangNhap").hide();
        $("#DangKy").hide();
        $("#LayMatKhau").hide();
    }
    function OpenLogin(){
        $("#Landing").hide();
        $("#DangNhap").show();
        $("#DangKy").hide();
        $("#LayMatKhau").hide();
        $("#FormLayMatKhau").show();
        $("#TabLayMatKhau").hide();
    }
    function OpenGetPass(){
        $("#Landing").hide();
        $("#DangNhap").hide();
        $("#DangKy").hide();
        $("#LayMatKhau").show();
    }
    function Register(){
        $("#Loading").show();
        var phone_value = $("#reg_phone").val();
            phone = phone_value.replace(/\s+/g, ''); 
            name = $("#reg_name").val();
            pass = $("#reg_pass").val();
            pass2 = $("#reg_pass2").val();
        if (phone == ""||name == ""||pass == ""||pass2 == "") {
        Swal.fire({
            title: "Thất bại",
            text: "Vui lòng nhập đầy đủ thông tin!",
            icon: "error"
        });
        $("#Loading").hide();
        return false;
        }
        if (pass != pass2) {
        Swal.fire({
            title: "Thất bại",
            text: "Mật khẩu nhập lại không trùng khớp với mật khẩu bạn nhập trước đó!",
            icon: "error"
        });
        $("#Loading").hide();
        return false;
        }
        $.ajax({
        dataType: "json",
        url: "/DangKy",
        method: "POST",
        data: {phone:phone, name:name, password:pass},
        success : function(response){
            var data = response;
            if (data['status'] == false) {
            Swal.fire({
                title: "Thất bại",
                text: data["message"],
                icon: "error"
            });
            $("#Loading").hide();
            return false;
            }else if (data['status'] == true) {
            Swal.fire({
                title: "Thành công",
                text: data["message"],
                icon: "success"
            });
            $("#Loading").hide();
            setTimeout(function(){window.location.href = "/";}, 100);
            }
        },
        error: function(xhr, status, error) {
            console.log(error);
            Swal.fire({
            title: "Đã xảy ra lỗi",
            text: "Thử lại sau ít phút nếu vẫn còn lỗi hãy liên hệ quản trị viên để được hỗ trợ!",
            icon: "error"
            });
            $("#Loading").hide();
            return false;
        }
        });
    };
    function Login(){
        $("#Loading").show();
        var phone_value = $("#login_phone").val();
            phone = phone_value.replace(/\s+/g, ''); 
            pass = $("#login_pass").val();
        if (phone == ""||pass == "") {
        Swal.fire({
            title: "Thất bại",
            text: "Vui lòng nhập đầy đủ thông tin!",
            icon: "error"
        });
        $("#Loading").hide();
        return false;
        }
        $.ajax({
        dataType: "json",
        url: "/DangNhap",
        method: "POST",
        data: {phone:phone, password:pass},
        success : function(response){
            var data = response;
            if (data['status'] == false) {
            Swal.fire({
                title: "Thất bại",
                text: data["message"],
                icon: "error"
            });
            $("#Loading").hide();
            return false;
            }else if (data['status'] == true) {
            Swal.fire({
                title: "Thành công",
                text: data["message"],
                icon: "success"
            });
            $("html, body").animate({scrollTop: 0}, 0);
            $("#Loading").hide();
            setTimeout(function(){window.location.href = "/";}, 100);
            }
        },
        error: function(xhr, status, error) {
            console.log(error);
            Swal.fire({
            title: "Đã xảy ra lỗi",
            text: "Thử lại sau ít phút nếu vẫn còn lỗi hãy liên hệ quản trị viên để được hỗ trợ!",
            icon: "error"
            });
            $("#Loading").hide();
            return false;
        }
        });
    };
    function CheckPhone(){
        $("#Loading").show();
        var phone = $("#get_pass_phone").val();
        if (phone == "") {
            Swal.fire({
                title: "Thất bại",
                text: "Vui lòng nhập đầy đủ thông tin!",
                icon: "error"
            });
            $("#Loading").hide();
            return false;
        }
        $.ajax({
            dataType: "json",
            url: "/CheckPhone",
            method: "POST",
            data: {phone:phone},
            success : function(response){
                var data = response;
                if (data['status'] == false) {
                    Swal.fire({
                        title: "Thất bại",
                        text: data["message"],
                        icon: "error"
                    });
                    $("#Loading").hide();
                    return false;
                }else if (data['status'] == true) {
                    $("#Loading").hide();
                    $("#FormLayMatKhau").hide();
                    $("#TabLayMatKhau").show();
                    $("#user_phone").html(phone);
                    $("#CuPhapLayMK").html(phone+"_LAYMK_LIKETIK");
                    $("#CuPhapLayMK").attr("onclick", "copy('"+phone+"_LAYMK_LIKETIK')");
                    $(".sms").attr("href", "sms:0339888626?body="+phone+"_LAYMK_LIKETIK")
                    if(data['verify'] == "zalo"){
                        $("#WithZALO").addClass("active");
                        $("#TabZALO").show();
                    }else{
                        $("#WithTELE").addClass("active");
                        $("#TabTELE").show();
                        $('#WithZALO').closest('li').hide();
                    }
                }
            },
            error: function(xhr, status, error) {
                console.log(error);
                Swal.fire({
                title: "Đã xảy ra lỗi",
                text: "Thử lại sau ít phút nếu vẫn còn lỗi hãy liên hệ quản trị viên để được hỗ trợ!",
                icon: "error"
                });
                $("#Loading").hide();
                return false;
            }
        });
    };
