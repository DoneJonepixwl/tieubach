    function BaoHanh(id_order){
        $("#Loading").show();
        $.ajax({
            dataType: "json",
            url: "/BaoHanh",
            method: "POST",
            data: {order_id:id_order},
            success : function(response){
                var data = response;
                if (data['status'] == false) {
                    if (data["login"] == false) {
                        $("#Loading").hide();
                        $("body").append(`<div id="alert_login" class="swal2-container swal2-center swal2-backdrop-show" style="overflow-y:auto"><div aria-labelledby="swal2-title" aria-describedby="swal2-html-container" class="swal2-popup swal2-modal swal2-icon-error swal2-show" tabindex="-1" role="dialog" aria-live="assertive" aria-modal="true" style="display:grid"><div class="swal2-icon swal2-error swal2-icon-show" style="display:flex"><span class="swal2-x-mark"><span class="swal2-x-mark-line-left"></span><span class="swal2-x-mark-line-right"></span></span></div><h2 class="swal2-title" id="swal2-title" style="display:block">Lỗi!</h2><div class="swal2-html-container" style="display:block">Bạn chưa đăng nhập!</div><input class="swal2-input" style="display:none"><div class="swal2-actions"><div class="swal2-loader"></div><button type="button" class="swal2-confirm swal2-styled" aria-label="" style="display:inline-block" onclick='$("#alert_login").hide()'>OK</button></div></div></div>`);
                        setTimeout(function() {
                            window.location.href = "/"
                        }, 1e3);
                        return;
                    }
                    ThongBao("error", "Thất bại", data["message"]);
                    $("#Loading").hide();
                    return false;
                }else if (data['status'] == true) {
                    ThongBao("success", "Thành công", data["message"]);
                    $("#Loading").hide();
                }
            },
            error: function(xhr, status, error) {
                console.log(error);
                ThongBao("error", "Lỗi hệ thống", "Liên hệ quản trị viên để được hỗ trợ!");
                $("#Loading").hide();
                return false;
            }
        })
    }
