    $(document).ready(function () {
        if (window.matchMedia('(min-width: 991.98px)').matches) {
            $("body").addClass("menu-open");
            $("html").addClass("menu-open");
        }
        $('#data_order').DataTable({
            bLengthChange: false,
            bFilter: false,
            paging: true,
            ordering: false,
            info: false,
        });
        $(".LoadPage").click(function(){ // load page
            $("#Landing").show();
            $("#mainTaiHoang").html("");
            $("#Loading").show();
            $('.LoadPage').removeClass("active");
            $('.dropdown-menu').removeClass("show");
            $('.dropdown-toggle').removeClass("show");
            $('.nav-item').removeClass("show");

            var TH = $(this).attr("TH");
                fileload = TH;
                title = $(this).attr("title");
                DV = $(this).attr("DV");
            setTimeout(() => {
                $('.'+DV).addClass("show");
            }, "1");
            document.querySelectorAll(`a[TH="${TH}"]`).forEach(el => {
                el.classList.add('active');
            });
            if(DV == "TikTok"){
                logo_title = '<h6><i class="fab fa-tiktok" aria-hidden="true"></i> '+title+'</h6>';
            }else if(DV == "Facebook"){
                logo_title = '<h6><i class="fab fa-facebook" aria-hidden="true"></i> '+title+'</h6>';
            }else if(DV == "Instagram"){
                logo_title = '<h6><i class="fab fa-instagram" aria-hidden="true"></i> '+title+'</h6>';
            }else if(DV == "iTok"){
                logo_title = '<img src="../assets/img/logo.png" alt=""><h5>'+title+'</h5>';
            }else{
                logo_title = '<h6>'+title+'</h6>';
            }
            $("#mainTaiHoang").load(fileload, function( response, status, xhr ) {
                if ( status == "error" ) {
                    $("#Loading").hide();
                    // ThongBao("error", "Thất bại!", "Đã có lỗi xảy ra, thử lại sau!");
                    ThongBao("error", "Thất bại!", title+" đang bảo trì và sẽ sớm mở lại, thử lại sau!");
                    return;
                }
                window.loaded = true;
                if (window.matchMedia('(max-width: 991.98px)').matches) {
                    $(".menu-open").removeClass("menu-open");
                }
                $("#TieuDe").html(logo_title);
                $("#Loading").hide();
            });
        });
            });
